{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended" , "plugin:prettier/recommended"],
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "no-const-assign": "off",
    "@typescript-eslint/no-explicit-any" : "off",
    "prettier/prettier": [
    "error",
  {
    "endOfLine": "auto",
    "singleQuote": true,
    "doubleQuote": true
  }
]
  },
  "env": {
    "browser": true,
    "es2021": true
  }
}